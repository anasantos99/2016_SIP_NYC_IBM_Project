function behavior() {
	document.getElementById("behavior").innerHTML = "How is my child behaving?";
}
function hw() {
	document.getElementById("hw").innerHTML = "Has my child given in all of the homework assignments?";
}
function problem() {
	document.getElementById("problem").innerHTML = "Is there a problem with my child in class?";
}
function extra() {
	document.getElementById("extra").innerHTML = "Is there something my child can do to improve their grade?";
}
function repeat() {
	document.getElementById("repeat").innerHTML = "Could you repeat the question?";
}
function home() {
	document.getElementById("home").innerHTML = "How can I help my child at home?";
}
